[user]
  name = indexzero
  email = charlie.robbins@gmail.com
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[color "status"]
  added = green
  changed = yellow
  untracked = red
[core]
  excludesfile = ~/.gitignore
[alias]
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

[includeIf "gitdir:~/GitEC/"]
    path = ~/GitEC/.gitconfig
